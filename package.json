{
  "name": "ethereum-tx-decode",
  "version": "0.0.1",
  "author": {
    "name": "Andrew Schneider"
  },
  "homepage": "https://github.com/schnogz/ethereum-tx-decode",
  "bugs": {
    "url": "https://github.com/schnogz/ethereum-tx-decode/issues"
  },
  "description": "",
  "engines": {
    "node": ">= 10.14.0",
    "yarn": ">= 1.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    },
    "concurrent": false,
    "ignore": [
      "**/package.json"
    ]
  },
  "scripts": {
    "lint": "eslint --cache --fix './**/*.js'",
    "prettier": "prettier './**/*.js' --loglevel error --write",
    "predeploy": "npm install",
    "deploy": "NODE_ENV=production webpack-cli --config ./config/webpack.config.js --display-error-details",
    "postdeploy": "",
    "start": "NODE_ENV=development webpack-dev-server --config ./webpack.config.js --progress --colors --watch --devtool cheap-module-source-map",
    "vet": "yarn prettier && yarn lint"
  },
  "dependencies": {
    "@material-ui/core": "4.7.0",
    "@material-ui/icons": "4.5.1",
    "ramda": "0.26.1",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-inline-react-svg": "1.1.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-react-html-attrs": "2.1.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.5",
    "css-loader": "3.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "4.3.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.19.1",
    "standard": "12.0.1",
    "style-loader": "0.23.1",
    "thread-loader": "2.1.3",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-middleware": "3.7.2",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.2"
  }
}
